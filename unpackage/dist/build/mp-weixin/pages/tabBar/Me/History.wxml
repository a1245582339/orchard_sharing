<view class="uni-padding-wrap" bind:__l="__l"><van-button style="float:right;margin-top:10rpx;" size="mini" type="danger" data-event-opts="{{[['^click',[['empty']]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">清空历史</van-button><block wx:for="{{list}}" wx:for-item="item" wx:for-index="i" wx:key="i"><view class="history"><block wx:if="{{item.log.length}}"><van-cell title="{{item.date}}" data-com-type="wx"></van-cell></block><block wx:for="{{item.log}}" wx:for-item="_item" wx:for-index="index" wx:key="index"><van-card desc="{{_item.desc}}" title="{{_item.title}}" thumb="{{_item.thumb}}" data-event-opts="{{[['^tap',[['toDetail']]]]}}" data-com-type="wx" bind:tap="__e" vue-slots="{{['footer']}}"><view slot="footer"><van-button type="danger" size="mini" data-event-opts="{{[['^click',[['del',[i,index]]]]]}}" data-com-type="wx" bind:click="__e" vue-slots="{{['default']}}">删除</van-button></view></van-card></block></view></block></view>